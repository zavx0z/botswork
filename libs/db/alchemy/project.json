{
	"name": "db-alchemy",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "libs/db/alchemy/db_alchemy",
	"targets": {
		"lock": {
			"executor": "@nxlv/python:run-commands",
			"options": {
				"command": "poetry lock --no-update",
				"cwd": "libs/db/alchemy"
			}
		},
		"add": {
			"executor": "@nxlv/python:add",
			"options": {}
		},
		"update": {
			"executor": "@nxlv/python:update",
			"options": {}
		},
		"remove": {
			"executor": "@nxlv/python:remove",
			"options": {}
		},
		"build": {
			"executor": "@nxlv/python:build",
			"outputs": ["{projectRoot}/dist"],
			"options": {
				"outputPath": "libs/db/alchemy/dist",
				"publish": false,
				"lockedVersions": true,
				"bundleLocalDependencies": true
			}
		},
		"install": {
			"executor": "@nxlv/python:install",
			"options": {
				"silent": false,
				"args": "",
				"cacheDir": ".cache/pypoetry",
				"verbose": false,
				"debug": false
			}
		},
		"lint": {
			"executor": "@nxlv/python:flake8",
			"outputs": ["{workspaceRoot}/reports/libs/db/alchemy/pylint.txt"],
			"options": {
				"outputFile": "reports/libs/db/alchemy/pylint.txt"
			}
		},
		"test": {
			"executor": "@nxlv/python:run-commands",
			"outputs": ["{workspaceRoot}/reports/libs/db/alchemy/unittests", "{workspaceRoot}/coverage/libs/db/alchemy"],
			"options": {
				"command": "poetry run pytest tests/",
				"cwd": "libs/db/alchemy"
			}
		}
	},
	"tags": []
}

{
  "name": "screen-remote",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/screen-remote/src",
  "projectType": "library",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "xstate typegen \"src/**/*.ts?(x)\" --watch",
        "cwd": "libs/screen-remote"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "xstate typegen \"src/**/*.ts?(x)\"",
        "cwd": "libs/screen-remote"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/screen-remote/**/*.ts"
        ]
      }
    }
  },
  "tags": []
}

{
  "name": "@lib/secure",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/secure/src",
  "targets": {
    "dev": {
      "executor": "@zavx0z/sveltekit:dev",
      "options": {
        "port": 1111,
        "env": {
          "PUBLIC_HOST": "http://127.0.0.1:8000",
          "PUBLIC_XSTATE_DEBUG": true,
          "PUBLIC_XSTATE_PANEL": true,
          "PUBLIC_XSTATE_IFRAME": false
        }
      },
      "configurations": {
        "watch": {
          "watchDepthSvelteProjects": [],
          "typegenWatch": true
        },
        "watch-typegen": {
          "typegenWatch": true
        }
      }
    },
    "typegen": {
      "executor": "@zavx0z/machine:typegen"
    },
    "build": {
      "executor": "@zavx0z/sveltekit:build"
    }
  }
}

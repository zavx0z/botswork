{
  "name": "@lib/viz",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host 0.0.0.0",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "devDependencies": {
    "@lib/dev": "workspace:*",
    "@lib/code": "workspace:*",
    "@lib/editor": "workspace:*",
    "@lib/icons": "workspace:*",
    "@lib/theme": "workspace:*",
    "@lib/ui": "workspace:*",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.27.6",
    "@sveltejs/package": "^2.0.0",
    "@types/prismjs": "^1.26.2",
    "bun-types": "^1.0.6",
    "publint": "^0.1.9",
    "svelte": "^5.0.0-next.13",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@xstate/graph": "^1.4.2",
    "@xstate/svelte": "^2.1.0",
    "elkjs": "^0.8.2",
    "javascript-stringify": "^2.1.0",
    "prism-svelte": "^0.5.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "web-worker": "^1.2.0",
    "xstate": "beta"
  }
}

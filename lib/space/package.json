{
  "name": "@lib/space",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host 0.0.0.0 --port 4444",
    "build": "vite build && bunx run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "devDependencies": {
    "@lib/db": "workspace:*",
    "@lib/dev": "workspace:*",
    "@lib/code": "workspace:*",
    "@lib/editor": "workspace:*",
    "@lib/icons": "workspace:*",
    "@lib/theme": "workspace:*",
    "@lib/ui": "workspace:*",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.27.6",
    "@sveltejs/package": "^2.0.0",
    "publint": "^0.1.9",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-cross-origin-isolation": "^0.1.6",
    "vite-plugin-mkcert": "^1.16.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@dimforge/rapier3d-compat": "^0.11.2",
    "@theatre/studio": "^0.7.0",
    "@threlte/core": "^6.1.0",
    "@threlte/extras": "^7.3.0",
    "@threlte/rapier": "^1.1.2",
    "@threlte/theatre": "^2.1.3",
    "@tweakpane/core": "^2.0.1",
    "@types/three": "^0.157.2",
    "@xstate/svelte": "^3.0.0",
    "prismjs": "^1.29.0",
    "three": "^0.157.0",
    "tweakpane": "^3.1.0",
    "xstate": "^5.0.0"
  }
}

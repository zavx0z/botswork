{
  "name": "zavx0z/machine",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/machine/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tools/machine",
        "main": "tools/machine/src/index.ts",
        "tsConfig": "tools/machine/tsconfig.lib.json",
        "assets": [
          "tools/machine/*.md",
          {
            "input": "./tools/machine/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./tools/machine/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./tools/machine",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./tools/machine",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "tools/machine/**/*.ts",
          "tools/machine/package.json"
        ]
      }
    }
  },
  "tags": []
}

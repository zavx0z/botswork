{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": []
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  },
  "targetDefaults": {
    "sveltekit-dev": {
      "executor": "@zavx0z/sveltekit:dev",
      "options": {
        "port": 1000,
        "watchDepthSvelteProjects": "off"
      },
      "configurations": {
        "watch-all": {
          "watchDepthSvelteProjects": "all"
        }
      }
    },
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore",
        "{workspaceRoot}/eslint.config.js"
      ]
    }
  }
}

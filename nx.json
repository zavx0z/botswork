{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": []
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  },
  "targetDefaults": {
    "dev": {
      "executor": "@zavx0z/sveltekit:dev",
      "options": {
        "port": 1000,
        "watchDepthSvelteProjects": "off"
      },
      "configurations": {
        "watch-all": {
          "watchDepthSvelteProjects": "all"
        }
      }
    },
    "typegen": {
      "executor": "@zavx0z/machine:typegen",

      "configurations": {
        "watch": {
          "watch": true
        }
      }
    },
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore",
        "{workspaceRoot}/eslint.config.js"
      ]
    },
    "test": {
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/jest.preset.js"
      ]
    }
  },
  "plugins": [
    "@nxlv/python"
  ],
  "generators": {
    "@nxlv/python:poetry-project": {
      "pyenvPythonVersion": "3.11.5",
      "pyprojectPythonDependency": "~=3.11.5"
    },
    "@nxlv/python:migrate-to-shared-venv": {
      "pyenvPythonVersion": "3.11.5",
      "pyprojectPythonDependency": "~=3.11.5"
    }
  }
}

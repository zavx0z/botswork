{
  "name": "io-chat",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/io/chat/src",
  "projectType": "application",
  "targets": {
    "rebuild": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx env-cmd --silent -x -f ./.env docker-compose up --build -d io-chat",
        "parallel": true
      },
      "dependsOn": [
        "build"
      ]
    },
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "defaultConfiguration": "production",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "platform": "node",
        "outputPath": "apps/io/chat/dist",
        "format": [
          "cjs"
        ],
        "bundle": false,
        "main": "apps/io/chat/src/main.js",
        "tsConfig": "apps/io/chat/tsconfig.app.json",
        "assets": [
          "apps/io/chat/src/assets"
        ],
        "generatePackageJson": true,
        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          }
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "generateLockfile": true,
          "esbuildOptions": {
            "sourcemap": false,
            "outExtension": {
              ".js": ".js"
            }
          }
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "io-chat:build:development",
        "inspect": false
      }
    }
  },
  "tags": []
}
